
module munin_openstack_custom 1.0;

require {
	type ldconfig_exec_t;
	type commplex_main_port_t;
	type munin_plugin_state_t;
	type munin_t;
	class tcp_socket name_connect;
	class dir write;
	class file { execute getattr };
}

#============= munin_t ==============
allow munin_t commplex_main_port_t:tcp_socket name_connect;
allow munin_t ldconfig_exec_t:file { execute getattr };
allow munin_t munin_plugin_state_t:dir write;
